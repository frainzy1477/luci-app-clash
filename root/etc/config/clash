	option enable_ipv6 'false'
	option tun_mode '0'
	option enable '0'
	option dash_port '9090'
	option dash_pass '123456'
	option auto_update '0'	
	option auto_update_time '0'
	option level 'info'
	option core '1'
	option allow_lan 'true'
	option bind_addr '*'
	option dnsforwader '1'
